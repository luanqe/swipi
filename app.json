{
  "expo": {
    "name": "Swipi",
    "slug": "swipi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A66C2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.swipi.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A66C2"
      },
      "package": "com.swipi.app"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0A66C2",
          "image": "./assets/splash.png",
          "imageWidth": 200
        }
      ]
    ]
  }
}
